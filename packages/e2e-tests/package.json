{
  "name": "@uni-final/e2e-tests",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "End-to-end testing package for TrueNamePath university project",
  "scripts": {
"test": "playwright test",
"test:ui": "playwright test --ui --config=playwright.config.ui.ts",
"test:ui:setup": "playwright test --project=setup && playwright test --ui --config=playwright.config.ui.ts",
"test:ui:with-setup": "playwright test --project=setup && playwright test --ui --config=playwright.config.ui.ts",
"test:setup": "playwright test --project=setup",
"test:headed": "playwright test --headed",
"test:debug": "playwright test --debug",
"type-check": "tsc --noEmit",
"lint": "eslint .",
"lint:fix": "eslint . --fix"
  },
  "devDependencies": {
"@playwright/test": "^1.55.0",
"@supabase/supabase-js": "^2.56.0",
"@types/node": "24.3.0",
"dotenv": "^17.2.1",
"typescript": "^5.8.2"
  }
}
