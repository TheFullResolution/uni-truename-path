{
  "name": "@uni-final/truename-oauth",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Shared OAuth integration library for TrueNamePath demo applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
".": {
  "import": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
  },
  "scripts": {
"build": "tsc",
"type-check": "tsc --noEmit",
"lint": "eslint src --ext .ts,.tsx",
