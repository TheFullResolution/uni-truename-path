{
  "name": "@uni-final/truename-oauth",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Shared OAuth integration library for TrueNamePath demo applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
".": {
  "import": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
  },
  "scripts": {
"build": "tsc",
"type-check": "tsc --noEmit",
"lint": "eslint src --ext .ts",
"lint:fix": "eslint src --ext .ts --fix",
"clean": "rm -rf dist",
"test": "vitest run",
"test:watch": "vitest",
"test:coverage": "vitest run --coverage",
"test:ui": "vitest --ui"
  },
  "devDependencies": {
"@types/node": "24.3.0",
"@vitest/coverage-v8": "^2.0.0",
"@vitest/ui": "^2.0.0",
"typescript": "^5.8.2",
"vitest": "^2.0.0"
  },
  "files": [
"dist/**/*",
"README.md"
  ]
}
